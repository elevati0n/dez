<%= simple_form_for(@car) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :details %>
    <%= f.association :user %>
    <%= f.association :location %>
    <%= f.association :job %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
